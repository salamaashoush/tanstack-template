{
  "name": "tanstack-template",
  "version": "0.0.0",
  "description": "TanStack Template",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "pnpm with-env vinxi dev",
    "build": "pnpm with-env vinxi build",
    "start": "pnpm with-env vinxi start",
    "lint": "pnpm lint:i18n && eslint",
    "test": "pnpm with-env vitest",
    "coverage": "pnpm with-env vitest run --coverage",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e .env --",
    "prepare": "node .husky/install.mjs && tsx scripts/setup.ts",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e": "playwright test --config=playwright.config.ts",
    "test:e2e:ui": "playwright test --ui --config=playwright.config.ts",
    "lint:i18n": "npx -y @inlang/cli lint --project ./project.inlang --languageTags en,ar",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./app/i18n"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@tanstack/react-query": "^5.66.11",
    "@tanstack/react-query-devtools": "^5.66.11",
    "@tanstack/react-router": "^1.112.0",
    "@tanstack/react-router-with-query": "^1.112.0",
    "@tanstack/router-valibot-adapter": "^1.81.5",
    "@tanstack/start": "^1.112.1",
    "axios": "^1.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.477.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-cookie": "^7.2.2",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sonner": "^2.0.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.0.2",
    "valibot": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.21.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@inlang/paraglide-js": "1.11.8",
    "@inlang/paraglide-vite": "^1.4.0",
    "@playwright/test": "^1.50.1",
    "@tailwindcss/postcss": "^4.0.9",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "4.0.9",
    "@tanstack/eslint-plugin-router": "^1.105.0",
    "@tanstack/router-devtools": "^1.112.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/browser": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "happy-dom": "^17.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "playwright": "^1.50.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.9",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0",
    "vinxi": "^0.5.3",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.7",
    "vitest-browser-react": "^0.1.1",
    "vitest-dom": "^0.1.1"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@10.4.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "msw"
    ]
  }
}
